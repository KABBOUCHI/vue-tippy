{
  "name": "vue-tippy",
  "version": "4.11.0",
  "description": "Directive and Component wrapper for Tippy.js",
  "author": "Georges KABBOUCHI <georges.kabbouchi@gmail.com>",
  "files": [
    "dist",
    "src",
    "composition",
    "vetur"
