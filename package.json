{
  "name": "vue-tippy",
  "version": "6.5.0",
  "main": "index.js",
  "module": "dist/vue-tippy.mjs",
  "unpkg": "dist/vue-tippy.iife.js",
  "jsdelivr": "dist/vue-tippy.iife.js",
  "types": "dist/vue-tippy.d.ts",
  "exports": {
    ".": {
      "types": "./dist/vue-tippy.d.ts",
      "browser": "./dist/vue-tippy.esm-browser.js",
      "node": {
        "import": "./dist/vue-tippy.mjs",
        "require": {
          "production": "./dist/vue-tippy.prod.cjs",
          "development": "./dist/vue-tippy.cjs",
          "default": "./index.js"
        }
      },
      "import": "./dist/vue-tippy.mjs"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "src/**/*",
    "dist/*.js",
    "dist/*.mjs",
    "dist/*.cjs",
    "dist/vue-tippy.d.ts",
    "index.js",
    "index.cjs",
    "vetur/*.json",
    "tsconfig.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup -c rollup.config.js",
    "build:dts": "api-extractor run --local --verbose",
    "dev": "webpack-dev-server --mode=development",
    "dev:docs": "cd docs && yarn dev",
    "prepublishOnly": "yarn build && yarn build:dts"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.8.1",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/webpack": "^4.41.21",
    "@types/webpack-env": "^1.15.2",
    "@vue/compiler-sfc": "^3.2.0",
    "css-loader": "^4.2.0",
    "html-webpack-plugin": "^4.3.0",
    "rollup": "^2.23.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3",
    "vue": "^3.2.0",
    "vue-loader": "^16.0.0-beta.8",
    "vue-router": "4",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "tippy.js": "^6.3.7"
  },
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  }
}
